<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="com.example.patrick.radoctor.Biologics_Activity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        android:fillViewport="true">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:id="@+id/biologic1">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="10dp"
                        android:textSize="15sp"
                        android:textColor="#000075"
                        android:text="@string/label_inflix"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dose: "
                        android:id="@+id/doseInflix"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Infusion Number: "
                        android:id="@+id/infInflix"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Last Updated: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/luInflix"
                        android:textColor="#ff000000"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:id="@+id/biologic2">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#000075"
                        android:text="@string/label_inflix2"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dose: "
                        android:id="@+id/doseInflix2"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Infusion Number: "
                        android:id="@+id/infInflix2"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Last Updated: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/luInflix2"
                        android:textColor="#ff000000"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:id="@+id/biologic3">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#000075"
                        android:text="@string/label_ritux"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dose: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/doseRitux"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Infusion Number: "
                        android:id="@+id/infRitux"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Last Updated: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/luRitux"
                        android:textColor="#ff000000"/>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:id="@+id/biologic4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#000075"
                        android:text="@string/label_tocili"
                        android:id="@+id/labelTocili"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dose: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/doseTocili"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Infusion Number: "
                        android:id="@+id/infTocili"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Last Updated: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/luTocili"
                        android:textColor="#ff000000"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:id="@+id/biologic5">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#000075"
                        android:text="@string/label_etaner"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dose: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/doseEtaner"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Infusion Number: "
                        android:id="@+id/infEtaner"
                        android:textColor="#ff000000"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Last Updated: "
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:id="@+id/luEtaner"
                        android:textColor="#ff000000"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>


</RelativeLayout>
